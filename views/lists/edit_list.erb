<h2>Editting <%= @mlist.title %></h3><br />
<br />
<form method='post' action='/console/<%= @mlist._id %>/edit/'>
<b>Title:</b><br />
  <input type='text' name='title' value='<%= @mlist.title %>' /><br />
<b>Description:</b><br />
  <input type='text' name='desc' value='<%= @mlist.describe %>' /><br />
<% if @paid == true %>
  <b>Public? </b>
  <input type='checkbox' name='public' value='true' checked /><br />
<% end %>
<br />
<input type='submit' value='Edit' />
</form>
